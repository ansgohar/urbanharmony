{
  "connection": "default",
  "collectionName": "internalnews",
  "info": {
    "name": "internalnews",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "AuthorName": {
      "type": "string"
    },
    "ArticleName": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Type": {
      "type": "enumeration",
      "enum": [
        "Social",
        "Paper",
        "Electronic"
      ],
      "required": true
    },
    "FullArticle": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "datePublished": {
      "type": "date"
    }
  }
}