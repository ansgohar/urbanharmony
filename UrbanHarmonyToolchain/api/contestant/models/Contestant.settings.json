{
  "connection": "default",
  "collectionName": "contestant",
  "info": {
    "name": "contestant",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "age": {
      "type": "integer"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "personalPhoto": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "img1": {
      "type": "string"
    },
    "img2": {
      "type": "string"
    },
    "img3": {
      "type": "string"
    },
    "status": {
      "default": "applied",
      "type": "enumeration",
      "enum": [
        "applied",
        "accepted",
        "winner",
        "first",
        "second",
        "third"
      ]
    },
    "competitions": {
      "model": "competitions",
      "via": "contestants"
    },
    "address": {
      "type": "string"
    }
  }
}