    },
    "genID": {
      "type": "string"
    },
    "owner": {
      "type": "string",
      "required": true
    },
    "ownerType": {
      "type": "string"
    },
    "committeDesicion": {
      "type": "string"
    },
    "recommendation": {
      "type": "string"
    },
    "buildingPhoto": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}
